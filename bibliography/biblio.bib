@book{Amaral_2012,
  place = {Rio de Janeiro},
  edition = {3},
  title = {{Equipamentos mecânicos: Análise de falhas e solução de problemas}},
  isbn = {978-8541400367},
  abstractnote = {O objetivo do autor é capacitar engenheiros e técnicos da indústria em técnicas capazes de promover uma substancial redução do custo de manutenção e aumento da confiabilidade das máquinas existentes em uma instalação industrial. A obra é dividida em sete capítulos. No primeiro, são abordadas definições e exemplos de fontes de falhas na vida do equipamento, no segundo, a prática da análise de falhas; e no terceiro, a organização para prevenção de falhas. Nos Capítulos 4 e 5 são analisados os mecanismos de falha e a análise de falhas de componentes. já o sexto e sétimo explicam as falhas de equipamentos e oferecem dicas para uma boa manutenção.},
  journal = {Rio de Janeiro: Editora Qualitymark},
  author = {Amaral, Affonso L O},
  editor = {QualityMark},
  year = {2012},
}


@book{Wicker_Lewis_2015,
  edition = {5},
  title = {{Machine elements in mechanical design}},
  isbn = {9780791850657},
  abstractnote = {In previous works the authors presented two load distribution models for gears. One of the models was based on the rigid tooth assumption and the more complex of the two took advantage of influence coefficients obtained from a FEM model to find the load distribution. In this work an analytical model relying on the ISO 6336 maximum teeth stiffness and a parabolic single tooth stiffness per unit of single line length was developed. This load distribution model relies on an original description of the contact line length based on Heaviside functions to find the gear mesh stiffness. The proposed model is of straight forward implementation, very little computational cost and yields promising results. The concept of constant mesh stiffness gear design is also introduced.},
  publisher = {Pergamon},
  author = {Marques, Pedro and Martins, Ramiro and Seabra, Jorge},
  editor = {Pearson},
  year = {2017},
  issn = {0094-114X},
  journal = {Mechanism and Machine Theory},
  pages = {126–140},
  volume = {113},
  url = {https://www.sciencedirect.com/science/article/pii/S0094114X16306486},
  doi = {10.1016/J.MECHMACHTHEORY.2017.03.007},
  note = {12 citações em periódicos da área Periódico com: SNIP 2,67 SJR 1,317 IF 3,535  2/9},
  month = {Jul},
  number = {1–2},
  booktitle = {ASME International Mechanical Engineering Congress and Exposition, Proceedings (IMECE)},
}


